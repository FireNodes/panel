{
    "name": "@flowtr/panel-backend",
    "version": "0.0.1",
    "private": true,
    "description": "Flowtr panel backend",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "fdpla": "./dist/cli.js"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "scripts": {
        "cm": "cz",
        "build": "tsc",
        "dev": "nodemon --watch src --ext ts,js --exec \"pnpm run build && pnpm run start\"",
        "start": "node dist/cli.js run",
        "cli": "node dist/cli.js",
        "prepublishOnly": "pnpm run lint && pnpm run test && pnpm run build",
        "test": "c8 ava test/**/*.ts",
        "lint": "eslint '{src,test}/**/*.ts' --fix"
    },
    "keywords": [
        "typescript",
        "node"
    ],
    "author": "Theo Paris",
    "license": "MIT",
    "devDependencies": {
        "@types/bcrypt": "5.0.0",
        "@types/cors": "2.8.12",
        "@types/dockerode": "3.3.9",
        "@types/jsonwebtoken": "8.5.8",
        "@types/node": "15.12.1",
        "@types/yargs": "17.0.10",
        "@typescript-eslint/eslint-plugin": "4.28.2",
        "@typescript-eslint/parser": "4.28.2",
        "commitizen": "4.2.5",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "7.30.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "3.4.0",
        "nodemon": "2.0.19",
        "prettier": "2.7.1",
        "typescript": "4.7.4"
    },
    "dependencies": {
        "@fastify/cors": "^7.0.0",
        "@fastify/helmet": "^8.1.0",
        "@flowtr/panel-sdk": "workspace:^0.0.1",
        "axios": "0.27.2",
        "bcrypt": "5.0.1",
        "cors": "2.8.5",
        "dockerode": "3.3.2",
        "fastify": "^3.29.1",
        "fastify-plugin": "^3.0.1",
        "json5": "^2.2.1",
        "jsonwebtoken": "8.5.1",
        "middie": "^6.1.0",
        "nanoid": "3.3.4",
        "pg": "8.7.3",
        "tslog": "^3.3.3",
        "typeorm": "0.3.7",
        "yargs": "17.5.1",
        "zod": "3.17.10"
    }
}
