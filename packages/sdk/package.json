    "author": "Theo Paris",
    "license": "GPL-3.0-or-later",
    "devDependencies": {
        "@types/chai": "4.2.19",
        "@types/mocha": "8.2.2",
        "@types/node": "15.12.1",
        "@typescript-eslint/eslint-plugin": "4.28.0",
        "@typescript-eslint/parser": "4.28.0",
        "chai": "4.3.4",
        "commitizen": "4.2.4",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "7.29.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "3.4.0",
        "mocha": "9.0.1",
        "nyc": "15.1.0",
        "prettier": "2.3.2",
        "ts-mocha": "8.0.0",
        "typescript": "4.3.4"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "nanoid": "^3.1.23",
        "zod": "^3.2.0"
    }
